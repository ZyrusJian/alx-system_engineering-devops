#!/usr/bin/env bash
# This script stops the 4-to_infinity_and_beyond process.
# Get the PID of the 4-to_infinity_and_beyond process
pid=$(pgrep -f "4-to_infinity_and_beyond")
# Check if the process is running
if [ -z "$pid" ]; then
    echo "The 4-to_infinity_and_beyond process is not running."
else
    # Stop the process using the kill command
    kill "$pid"
    echo "The 4-to_infinity_and_beyond process has been stopped."
fi
